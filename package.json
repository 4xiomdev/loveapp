{
  "name": "loveapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "VITE_ENVIRONMENT=development vite",
    "emulators": "GOOGLE_APPLICATION_CREDENTIALS= firebase emulators:start --only auth,firestore,functions",
    "emulators:kill": "pkill -f \"firebase emulators\" || true",
    "dev:emulators": "npm run emulators:kill && GOOGLE_APPLICATION_CREDENTIALS= concurrently \"npm run emulators\" \"npm run dev:local\"",
    "build": "vite build",
    "build:dev": "VITE_ENVIRONMENT=development vite build",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/google-calendar": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@googleapis/calendar": "^9.7.9",
    "@mui/icons-material": "^5.15.7",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.15.7",
    "@mui/x-date-pickers": "^5.0.20",
    "@react-oauth/google": "^0.12.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "emoji-picker-react": "^4.12.0",
    "firebase": "^11.2.0",
    "firebase-admin": "^13.0.2",
    "framer-motion": "^12.4.7",
    "gapi-script": "^1.2.0",
    "googleapis": "^144.0.0",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.475.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.1",
    "react": "^18.3.1",
    "react-big-calendar": "^1.18.0",
    "react-confetti": "^6.2.2",
    "react-dom": "^18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.3",
    "react-window": "^1.8.11",
    "recharts": "^2.15.0",
    "tailwindcss": "^4.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "vite": "^6.0.5"
  }
}
